{
  "manifest_version": 2,
  "name": "Bookmarks extension",
  "description": "A simple bookmark extension to chrome",
  "version": "0.1",
  "permissions": [
    "bookmarks",
    "tabs",
    "chrome://favicon/"
  ],
  "omnibox": {
    "keyword":"bm"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "bookmark-omnibox.js"
    ]
  
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
